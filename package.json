{
  "name": "ride-my-way-frontend",
  "version": "1.0.0",
  "description": "A carpooling application using React",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "test": "jest --verbose --coverage --detectOpenHandles --forceExit",
    "test:watch": "jnpm test -- --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-beloved/Ride-my-way-frontend.git"
  },
  "keywords": [
    "React",
    "Redux",
    "Webpack",
    "SASS"
  ],
  "author": "D-Beloved",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/d-beloved/Ride-my-way-frontend/issues"
  },
  "homepage": "https://github.com/d-beloved/Ride-my-way-frontend#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^0.4.5",
    "jest": "^23.6.0",
    "nyc": "^13.1.0",
    "redux": "^4.0.1",
    "style-loader": "^0.23.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0"
